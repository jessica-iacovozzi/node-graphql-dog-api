{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/health(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
}